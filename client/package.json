{
  "name": "flask-urls",
  "version": "0.0.1",
  "description": "Provides a function to build Flask URLs on the client side.",
  "main": "lib/build.js",
  "scripts": {
    "build": "rm -rf lib/ && babel src/ --out-dir lib/",
    "prepare": "npm run build",
    "eslint": "eslint src/",
    "test": "jest --coverage"
  },
  "repository": "github:indico/js-flask-urls",
  "keywords": [
    "flask",
    "werkzeug",
    "url",
    "routing"
  ],
  "author": "Indico Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/indico/js-flask-urls/issues"
  },
  "homepage": "https://github.com/indico/js-flask-urls#readme",
  "dependencies": {
    "lodash": "^4.17.11",
    "qs": "^6.7.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "eslint": "^6.0.0",
    "eslint-config-indico": "^1.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "^22.7.1",
    "jest": "^24.8.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "firefox esr",
              "not ie<11",
              "not ie_mob>0"
            ]
          }
        }
      ]
    ]
  }
}
